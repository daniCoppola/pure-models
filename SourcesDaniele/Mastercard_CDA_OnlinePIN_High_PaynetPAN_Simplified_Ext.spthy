/* 
    Generates an epheremal DH value and sends it to the card. The state 
    is saved in Terminal_Ext_Sent_Hello.
*/
rule Terminal_Ext_Sends_Hello:
let
		Terminal_Eph_Pk = 'g'^~Terminal_Eph_Sk
	in 
	[ 
		Terminal_Sent_GPO($Terminal, PDOL), In(<AIP, 'AFL'>), Fr(~Terminal_Eph_Sk)
	] -->
	[	Terminal_Ext_Sent_Hello($Terminal, PDOL, AIP, < Terminal_Eph_Pk, ~Terminal_Eph_Sk>) , Out(<'EXT_HELLO', Terminal_Eph_Pk>)
  //,Ephemeral_Key_Terminal($Terminal, Terminal_Eph_Pk, ~Terminal_Eph_Sk)
  ]

/*
    Creates an ephemeral DH value, derives a shared secret with the terminal. 
    Computes a MAC over the EDH values and sends the MAC and EDH to the terminal.
*/
rule Card_Ext_Responds_To_Hello:
	let 
		Card_Eph_Pk = 'g'^~Card_Eph_Sk
		DH = <Card_Eph_Pk, Terminal_Eph_Pk, Terminal_Eph_Pk^~Card_Eph_Sk>
		mac = MAC(f(Terminal_Eph_Pk^~Card_Eph_Sk,'MAC_CARD_TO_READER'), <Card_Eph_Pk, Terminal_Eph_Pk>)
		EXT_HELLO = <'EXT_HELLO', Terminal_Eph_Pk>
	in
	[	In(EXT_HELLO),
		Card_Responded_To_GPO(~PAN, PDOL, ATC),
		Fr(~Card_Eph_Sk)
	]-->
	[
		Out(<'EXT_HELLO', Card_Eph_Pk, mac>),
		Card_Responded_To_Hello(~PAN, PDOL, ATC, DH) 
    //,Ephemeral_Key_Card(~PAN, Card_Eph_Pk, ~Card_Eph_Sk)
	]

/*
    Derives a shared secret with the card given the EDH save in the state
    and the EDH received from the card.
*/
rule Terminal_Ext_Receives_Hello:
	let
	mac_t = MAC(f(Card_Eph_Pk^~Terminal_Eph_Sk,'MAC_CARD_TO_READER'), <Card_Eph_Pk, Terminal_Eph_Pk>)
		edht = <Terminal_Eph_Pk, ~Terminal_Eph_Sk>
		DH = <Card_Eph_Pk, Terminal_Eph_Pk, Card_Eph_Pk^~Terminal_Eph_Sk>
		EXT_HELLO = <'EXT_HELLO', Card_Eph_Pk, mac_c>
	in
	[	
		Terminal_Ext_Sent_Hello($Terminal, PDOL, AIP, edht),
		In(EXT_HELLO)
	]--[Eq(mac_c, mac_t), Terminal_Ext_Sent_Hello_dbg($Terminal, PDOL, AIP, edht) ]->

	[
		Terminal_Ext_Received_Hello($Terminal, PDOL, AIP, DH)
	]

// END ADDITIONAL RULES FOR DH EXCHANGE 

rule Terminal_Sends_ReadRecord:
    [ Terminal_Ext_Received_Hello($Terminal, PDOL, AIP, DH)]
  -->
    [ Out(<'READ_RECORD', 'AFL'>),
      Terminal_Sent_ReadRecord($Terminal, PDOL, AIP, DH) ]

rule Card_Responds_To_ReadRecord_NotDDA:
    [ Card_Responded_To_Hello(~PAN, PDOL, ATC, DH),
      !AIP(~PAN, AIP),
      !Records(~PAN, records),
      In(<'READ_RECORD', 'AFL'>) ]
  --[ NEq(fst(AIP), 'DDA') ]->
    [ Out(records),
      Card_Ready_For_Cryptogram(~PAN, PDOL, ATC, DH) ]

